<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.DetailDataMhsActivity">

    <TextView
        android:id="@+id/tv_nama_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_recycler_view"
        android:padding="16dp"
        android:textStyle="bold"
        android:textColor="@color/bluePrimary"
        android:textSize="20dp"
        android:text="Nama"
        android:layout_marginTop="80dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_umur_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_recycler_view"
        android:padding="16dp"
        android:textStyle="bold"
        android:textColor="@color/bluePrimary"
        android:textSize="20dp"
        android:text="Umur"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_nama_detail" />

    <TextView
        android:id="@+id/tv_asal_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="@drawable/shape_recycler_view"
        android:padding="16dp"
        android:textStyle="bold"
        android:textColor="@color/bluePrimary"
        android:textSize="20dp"
        android:text="Asal Kampus"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_umur_detail" />

</androidx.constraintlayout.widget.ConstraintLayout>